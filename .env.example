# Newsletter Service Configuration
# Choose one: 'mock', 'mailchimp', 'firebase', 'custom'
VITE_NEWSLETTER_SERVICE=mock

# Mailchimp Configuration (if using Mailchimp)
VITE_MAILCHIMP_API_KEY=your_mailchimp_api_key_here
VITE_MAILCHIMP_LIST_ID=your_mailchimp_list_id_here
VITE_MAILCHIMP_SERVER_PREFIX=us1

# Custom API Configuration (if using custom backend)
VITE_NEWSLETTER_API_ENDPOINT=https://your-api.com/api

# Firebase Configuration (if using Firebase)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_PROJECT_ID=your_project_id
